{
    "name": "Sheringer - copy url with </title>",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Copy url with </title>",
    "icons": {
        "19": "images/19.png",
        "38": "images/38.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}
